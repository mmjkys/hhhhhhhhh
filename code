for i in range(1):
    for j in range(1):
        print("*", end = "")
    print()
for k in range(1):
    for o in range(2):
        print("*", end = "")
    print()
for k in range(1):
    for o in range(3):
        print("*", end = "")
    print()
for k in range(1):
    for o in range(4):
        print("*", end = "")
    print()
    
for k in range(2):
    for o in range(3):
        print("*", end = "")
    print()
